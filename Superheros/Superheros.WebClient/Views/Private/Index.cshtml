@{
    ViewData["Title"] = "Private";
}

<h1>@ViewData["Title"]</h1>

<p>This page is protected by <span class="badge badge-dark">[Authorize]</span> attribute and it shows user stored claims</p>

<p>You can logout on <a asp-area="" asp-controller="Private" asp-action="Logout">this page</a></p>

<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>Claim</th>
            <th>Value</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var claim in User.Claims)
    {
        <tr>
            <td>@claim.Type</td>
            <td>@claim.Value</td>
        </tr>
    }
    </tbody>
</table>
